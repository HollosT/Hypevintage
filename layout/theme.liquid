<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <!-- Roboto font -->
  <link href="https://fonts.googleapis.com/css2?family=Dosis:wght@200;300;400;500;600;700;800&display=swap" rel="stylesheet">
  {{ content_for_header }} <!-- Header hook for plugins -->
  {%- comment -%} Materials icons are being used for having icons https://fonts.google.com/icons  {%- endcomment -%}

  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
  {{ 'navigation.css' | asset_url | stylesheet_tag }}
  {{ 'featured-collections.css' | asset_url | stylesheet_tag }}
  {{ 'application.css' | asset_url | stylesheet_tag }}
  {{ 'category-selection.css' | asset_url | stylesheet_tag }}
  {{ 'introduction.css' | asset_url | stylesheet_tag }}
  {{ 'slideshow.css' | asset_url | stylesheet_tag }}
  {{ 'faq.css' | asset_url | stylesheet_tag }}
  {{ 'cart.css' | asset_url | stylesheet_tag }}
  {{ 'footer.css' | asset_url | stylesheet_tag }}
  {{ 'shipping.css' | asset_url | stylesheet_tag }}
  {{ 'collection.css' | asset_url | stylesheet_tag }}
  {{ 'pagination.css' | asset_url | stylesheet_tag }}
  
  <script src="https://unpkg.com/vue@3.0.5"></script> 
  
  </head>
  <body>
    
    {% section 'header' %}
    
    <main role="main">
      {{ content_for_layout }}
    </main>

    {% section 'footer-section' %}

</body>
</html>



